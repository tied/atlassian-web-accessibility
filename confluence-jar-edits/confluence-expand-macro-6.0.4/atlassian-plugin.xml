<atlassian-plugin key="com.atlassian.confluence.plugins.expand-macro" name="Confluence Expand Macro" pluginsVersion="2"><plugin-info><description>Create an expandable block of text.</description><version>6.0.4</version><vendor name="Atlassian" url="http://www.atlassian.com/"/></plugin-info><component-import name="I18N Resolver" key="i18nResolver" interface="com.atlassian.sal.api.message.I18nResolver"/><resource name="i18n" type="i18n" location="com/atlassian/confluence/plugins/expand/i18n/i18n"/><resource name="images/" type="download" location="com/atlassian/confluence/plugins/expand/images"/><web-resource key="expand-macro-css-only" name="Core Expand Macro CSS Resource"><resource type="download" name="expand-macro.css" location="com/atlassian/confluence/plugins/expand/css/expand-macro.css"/></web-resource><web-resource key="expand-macro-core" name="Core Expand Macro Resources"><resource type="download" name="expand-macro-core.js" location="com/atlassian/confluence/plugins/expand/js/expand-macro-core.js"/><dependency>com.atlassian.confluence.plugins.expand-macro:expand-macro-css-only</dependency></web-resource><web-resource key="expand-macro-desktop-resources" name="Expand Macro Resources for Desktop"><resource type="download" name="expand-macro.js" location="com/atlassian/confluence/plugins/expand/js/expand-macro.js"/><dependency>com.atlassian.confluence.plugins.expand-macro:expand-macro-core</dependency><context>atl.confluence.macros.expand.desktop</context><context>viewcontent</context><context>preview</context></web-resource><web-resource key="expand-macro-mobile-resources" name="Expand Macro Resources for Mobile"><resource type="download" name="expand-macro-mobile.js" location="com/atlassian/confluence/plugins/expand/js/expand-macro-mobile.js"/><dependency>com.atlassian.confluence.plugins.expand-macro:expand-macro-core</dependency><context>atl.confluence.macros.expand.mobile</context></web-resource><web-resource key="expand-macro-soy-templates" name="Expand Macro Soy Templates"><resource type="soy" name="expand-macro.soy" location="com/atlassian/confluence/plugins/expand/templates/expand-macro.soy"/></web-resource><macro key="expand-migration" name="expand" class="com.atlassian.confluence.plugins.expand.ExpandV2Macro"/><xhtml-macro key="expand" name="expand" class="com.atlassian.confluence.plugins.expand.ExpandMacro" icon="/download/resources/com.atlassian.confluence.plugins.expand-macro/images/expand.png" documentation-url="http://confluence.atlassian.com/display/DOC/Expand+Macro"><description>Embeds an expandable text box into your page.</description><device-type>mobile</device-type><category name="formatting"/><parameters><parameter name="title" type="string" default="Click here to expand..."><option key="showNameInPlaceholder" value="false"/><option key="showValueInPlaceholder" value="true"/></parameter></parameters></xhtml-macro><analytics-whitelist key="analytics-whitelist" resource="com/atlassian/confluence/plugins/expand/analytics/whitelist.json"/></atlassian-plugin>